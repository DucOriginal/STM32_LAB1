#include "fsm_automatic.h"

void fsm_automatic_run()
{
	switch(status1){
		case INIT:
			status1 = AUTO_RED;
			setTimer1(counter_red);
			break;
		case AUTO_RED:
			break;
		case AUTO_YEL:
			break;
		case AUTO_GRE:
			break;
		default:
			break;

	}

	switch(status2){
		case INIT:
			status2 = AUTO_RED;
			setTimer1(500);
			break;
		case AUTO_RED:
			break;
		case AUTO_YEL:
			break;
		case AUTO_GRE:
			break;
		default:
			break;

	}
}
